    "family": "medangularapptask",
    "containerDefinitions": [
      {
        "image": "meanmedapprepo:latest",
        "name": "medangularappcontainer",
        "cpu": 10,
        "memoryReservation": 300,
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 4200,
            "protocol": "tcp"
          }
        ]
      }
    ]
  }